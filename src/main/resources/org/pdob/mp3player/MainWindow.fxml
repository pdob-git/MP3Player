<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="184.0" prefWidth="719.0" style="-fx-background-color: #222222;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.pdob.mp3player.MainWindow">
   <children>
      <Label fx:id="songLabel" alignment="CENTER" layoutX="-6.0" layoutY="1.0" prefHeight="124.0" prefWidth="727.0" text="MP3 PLayer" textFill="#15bf18">
         <font>
            <Font size="67.0" />
         </font>
      </Label>
      <ProgressBar fx:id="songProgressBar" layoutY="123.0" prefHeight="20.0" prefWidth="721.0" progress="0.0" />
      <Button fx:id="playButton" layoutX="-2.0" layoutY="143.0" mnemonicParsing="false" onAction="#playMedia" prefHeight="41.0" prefWidth="86.0" text="PLAY">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="resetButton" layoutX="170.0" layoutY="143.0" mnemonicParsing="false" onAction="#resetMedia" prefHeight="41.0" prefWidth="86.0" text="RESET">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="pauseButton" layoutX="84.0" layoutY="143.0" mnemonicParsing="false" onAction="#pauseMedia" prefHeight="41.0" prefWidth="86.0" text="PAUSE">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="previousButton" layoutX="256.0" layoutY="143.0" mnemonicParsing="false" onAction="#previousMedia" prefHeight="41.0" prefWidth="100.0" text="PREVIOUS">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="nextButton" layoutX="356.0" layoutY="143.0" mnemonicParsing="false" onAction="#nextMedia" prefHeight="41.0" prefWidth="100.0" text="NEXT">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <ComboBox fx:id="speedBox" layoutX="456.0" layoutY="143.0" onAction="#changeSpeed" prefHeight="41.0" prefWidth="100.0" promptText="SPEED" />
      <Slider fx:id="volumeSlider" layoutX="567.0" layoutY="156.0" max="100.0" value="50.0" />
   </children>
</AnchorPane>
